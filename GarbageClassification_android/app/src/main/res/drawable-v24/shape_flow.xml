<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
       android:shape="rectangle">

    <!--矩形填充颜色-->
    <solid
        android:color="@android:color/white"/>

    <!--矩形四个角的弧度-->
    <corners
        android:radius="20dp"/>

    <!--矩形边框宽度和颜色-->
    <stroke
        android:width="1sp"
        android:color="@android:color/darker_gray"/>

</shape>